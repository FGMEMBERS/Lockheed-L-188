<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<path>vsi.ac</path>

	<params>
		<indicated-speed-fpm>instrumentation/vertical-speed-indicator/indicated-speed-fpm</indicated-speed-fpm>
	</params>

	<animation>
		<type>material</type>
		<object-name>Face</object-name>
		<object-name>Needle</object-name>
		<emission>
			<red>0.5</red>
			<green>0.5</green>
			<blue>0.273</blue>
			<factor-prop>controls/lighting/instruments-norm</factor-prop>
		</emission>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>Needle</object-name>
		<property alias="../../params/indicated-speed-fpm" />
		<interpolation>
			<entry><ind>-6000</ind><dep>170</dep></entry>
			<entry><ind>-4000</ind><dep>135</dep></entry>
			<entry><ind>-2000</ind><dep>90</dep></entry>
			<entry><ind>-1000</ind><dep>45</dep></entry>
			<entry><ind>-500</ind><dep>22.5</dep></entry>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>500</ind><dep>-22.5</dep></entry>
			<entry><ind>1000</ind><dep>-45</dep></entry>
			<entry><ind>2000</ind><dep>-90</dep></entry>
			<entry><ind>4000</ind><dep>-135</dep></entry>
			<entry><ind>6000</ind><dep>-170</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

</PropertyList>
