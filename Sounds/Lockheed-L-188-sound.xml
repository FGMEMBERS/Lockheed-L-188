    <rumble>
        <name>rumble</name>
        <mode>looped</mode>
        <path>Sounds/rumble.wav</path>
        <condition>
            <or>
                <property>/gear/gear[0]/wow</property>
                <property>/gear/gear[1]/wow</property>
                <property>/gear/gear[2]/wow</property>
            </or>
        </condition>
        <volume>
            <property>velocities/groundspeed-kt</property>
            <factor>0.005</factor>
        </volume>
        <position>
            <x>-9.0</x>
            <y>0</y>
            <z>-1.68</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </rumble>

    <gear>
        <name>gear</name>
        <mode>in-transit</mode>
        <path>Aircraft/Lockheed-L-188/Sounds/Trains.wav</path>
        <property>/gear/gear[0]/position-norm</property>
        <pitch>
            <internal>dt_play</internal>
            <offset>1.0</offset>
            <factor>-0.001</factor>
        </pitch>
        <position>
            <x>-9.0</x>
            <y>0</y>
            <z>-1.68</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </gear>

    <gear>
        <name>gear-lock</name>
        <path>Aircraft/Lockheed-L-188/Sounds/lock.wav</path>
        <condition>
            <equals>
                <property>/gear/gear[0]/position-norm</property>
                <value>0</value>
            </equals>
        </condition>
        <position>
            <x>-9.0</x>
            <y>0</y>
            <z>-1.68</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </gear>

    <squeal>
        <name>squeal</name>
