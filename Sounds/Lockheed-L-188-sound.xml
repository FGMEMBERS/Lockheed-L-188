<?xml version="1.0"?>
<PropertyList>

<fx>

<!-- Engine 1 -->
    <crank>
        <name>crank1</name>
        <mode>looped</mode>
        <path>Aircraft/Lockheed-L-188/Sounds/starter.wav</path>
        <condition>
            <property>/engines/engine[0]/cranking</property>
            <not>
                <property>/engines/engine[0]/running</property>
            </not>
        </condition>
        <volume>
            <property>/sim/sound/view-volume</property>
        </volume>
        <position><!-- +X = back / +Y = left / +Z = up-->
            <x>-3.10</x>
            <y>8.35</y>
            <z>-0.3</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </crank>

    <crank>
        <name>cough1</name>
        <path>Aircraft/Lockheed-L-188/Sounds/turbine_start.wav</path>
        <property>/engines/engine[0]/running</property>
        <volume>
            <property>/sim/sound/view-volume</property>
        </volume>
        <position>
            <x>-3.10</x>
            <y>8.35</y>
            <z>-0.3</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>50.0</max-dist>
    </crank>

    <engine>
        <name>engine1_idle</name>
        <mode>looped</mode>
        <path>Aircraft/Lockheed-L-188/Sounds/props_running_2.wav</path>
        <condition>
            <and>
                <property>/engines/engine[0]/running</property>
                <less-than>
                    <property>/engines/engine[0]/rpm</property>
                    <value>1000</value>
                </less-than>
            </and>
        </condition>
        <volume>
            <property>/sim/sound/view-volume</property>
        </volume>
        <pitch>
            <property>/engines/engine[0]/bmep</property>
            <factor>0.004</factor>
            <offset>0.5</offset>
        </pitch>
        <position>
            <x>-3.10</x>
            <y>8.35</y>
            <z>-0.3</z>
        </position>
        <reference-dist>20.0</reference-dist>
        <max-dist>50.0</max-dist>
    </engine>

    <engine>
        <name>engine1_running</name>
        <mode>looped</mode>
        <path>Aircraft/Lockheed-L-188/Sounds/turboprop1.wav</path>
        <condition>
            <and>
                <property>/engines/engine[0]/running</property>
                <greater-than>
                    <property>/engines/engine[0]/rpm</property>
                    <value>999.999</value>
                </greater-than>
            </and>
        </condition>
        <volume>
            <property>engines/engine[0]/bmep</property>
            <factor>0.004</factor>
            <offset>0.25</offset>
        </volume>
        <volume>
            <property>/sim/sound/view-volume</property>
        </volume>
        <pitch>
            <property>/engines/engine[0]/bmep</property>
            <offset>0.4</offset>
            <factor>0.0035</factor>
        </pitch>
        <position>
            <x>-3.10</x>
            <y>8.35</y>
            <z>-0.3</z>
        </position>
        <reference-dist>150.0</reference-dist>
        <max-dist>500.0</max-dist>
    </engine>

