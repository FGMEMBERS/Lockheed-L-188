<?xml version="1.0"?>

<PropertyList>

    <sim>

        <description>Lockheed L-188A Electra (YASim)</description>
        <author>niltondc (3D Model), Martien van der Plas; David Waggoner: Flightdeck instrument installation</author>
        <aircraft-version>1.0.0</aircraft-version>
        <flight-model>yasim</flight-model>
        <aero>L-188</aero>
        <fuel-fraction>0.8</fuel-fraction>
        <startup>
            <splash-texture>Aircraft/Lockheed-L-188/USATour-splash-electra188.png</splash-texture>
        </startup>

        <sound>
            <path>Aircraft/Lockheed-L-188/Sounds/Lockheed-L-188-sound.xml</path>
        </sound>

        <model>
		 
            <path>Aircraft/Lockheed-L-188/Models/L-188.xml</path>
            <livery>
                <file type="string">default</file>
            </livery>

            <autostart type="bool">0</autostart>
	  <L-188>
	    <doors>
              <door>
              <position-norm type="double">0.0</position-norm>
              <name type="string">crew</name>
              </door>
	      <door>
              <position-norm type="double">0.0</position-norm>
              <name type="string">passenger</name>
              </door>
            </doors>
	  </L-188>
        </model>

        <remote>
         <pilot-callsign type="string"/>
        </remote>

        <allow-toggle-cockpit archive="y">true</allow-toggle-cockpit>

        <airport>
            <runways>
                <start-offset-m>50</start-offset-m>
            </runways>
        </airport>

        <help include="Help/Lockheed-L-188-help.xml"/>

        <instrumentation>
          <path>Aircraft/Lockheed-L-188/Systems/dc6-instrumentation.xml</path>
        </instrumentation>

	


        <view>
            <internal archive="y">true</internal>
            <config>
                <!-- x/y/z == right/up/back -->
                <x-offset-m archive="y">-0.57</x-offset-m>
                <y-offset-m archive="y">1.39</y-offset-m>
                <z-offset-m archive="y">-9.05</z-offset-m>
                <pitch-offset-deg>-12</pitch-offset-deg>
                <limits>
                    <enabled type="bool">true</enabled>
                    <left>
                        <heading-max-deg type="double">140</heading-max-deg>
                        <x-offset-max-m type="double">0.4</x-offset-max-m>
                        <x-offset-threshold-deg type="double">45</x-offset-threshold-deg>
                    </left>
                    <right>
                        <heading-max-deg type="double">160</heading-max-deg>
                        <x-offset-max-m type="double">0.20</x-offset-max-m>
                        <x-offset-threshold-deg type="double">65</x-offset-threshold-deg>
                    </right>
                </limits>
            </config>
        </view>

        <view n="100">
          <name>Engineer View</name>
          <enabled type="bool">true</enabled>
          <type>lookfrom</type>
          <internal type="bool">true</internal>
          <config>
            <from-model type="bool">true</from-model>
            <from-model-idx type="int">0</from-model-idx>
            <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
            <default-field-of-view-deg type="double">45.0</default-field-of-view-deg>
            <default-pitch-deg type="double">0</default-pitch-deg>
            <default-heading-deg type="double">0</default-heading-deg>
            <front-direction-deg type="double">0</front-direction-deg>
            <front-left-direction-deg type="double">45</front-left-direction-deg>
            <left-direction-deg type="double">90</left-direction-deg>
            <back-left-direction-deg type="double">135</back-left-direction-deg>
            <back-direction-deg type="double">180</back-direction-deg>
            <back-right-direction-deg type="double">225</back-right-direction-deg>
            <right-direction-deg type="double">270</right-direction-deg>
            <front-right-direction-deg type="double">315</front-right-direction-deg>
            <dynamic-view type="bool">true</dynamic-view>
                  <x-offset-m archive="y">0</x-offset-m>
                  <y-offset-m archive="y">1.25</y-offset-m>
                  <z-offset-m archive="y">-8.9</z-offset-m>
            <pitch-offset-deg>-18</pitch-offset-deg>
            <limits>
              <enabled type="bool">false</enabled>
            </limits>
          </config>
        </view>

        <view n="101">   
	  <name>Lounge View</name>   
	  <type>lookfrom</type>   
	  <internal archive="y">true</internal>   
	  <config>    
	    <from-model type="bool">true</from-model>    
	    <from-model-idx type="int">0</from-model-idx>
	    <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>    
	    <pitch-offset-deg>-5</pitch-offset-deg>    
	    <x-offset-m archive="y"> 0.0 </x-offset-m>      
	    <!-- Right -->    
	    <y-offset-m archive="y"> 1.500 </y-offset-m>    
	    <!-- Up -->    
	    <z-offset-m archive="y"> 14.572 </z-offset-m>    
	    <!-- Back -->    
            <pitch-offset-deg> -20 </pitch-offset-deg>    
	    <default-field-of-view-deg> 70 </default-field-of-view-deg>   
	  </config>  
	</view>   
	    
	<view n="102">   
	  <name>Wing view</name>    
	  <type>lookfrom</type>   
	  <internal archive="y">true</internal>   
	  <config>    
	    <from-model type="bool">true</from-model>    
	    <from-model-idx type="int">0</from-model-idx>    
	    <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>    
	    <pitch-offset-deg>-5</pitch-offset-deg>    
	    <x-offset-m archive="y"> -0.905 </x-offset-m>      
	    <!-- Right -->    
	    <y-offset-m archive="y"> 1.02137 </y-offset-m>    
	    <!-- Up -->    <z-offset-m archive="y"> 2.878 </z-offset-m>    
	    <!-- Back -->    <pitch-offset-deg> -20 </pitch-offset-deg>    
	    <default-field-of-view-deg> 70 </default-field-of-view-deg>   
	  </config>  
	</view>   
	    
	<chase-distance-m type="double" archive="y">-50.0</chase-distance-m>

        <multiplay>
          <generic>
	    <int n="0" type="int">0</int>  <!-- Dual Control boolean switches-->
	    <int n="2" alias="/controls/lighting/beacon-state/state"/>			<!-- Nav light - white becon -->
	    <int n="3" alias="/controls/lighting/strobe-state/state"/>			<!-- Nav light - red becon -->
	    <int n="4" alias="/controls/lighting/landing-lights"/>			<!-- Landing light -->
	    <int n="5" alias="/controls/lighting/nav-lights"/>				<!-- Nav light - top, bottom, left, right, back -->
            <float n="0">0</float>        <!-- for the shadow -->
            <float n="1">0</float>        <!-- for the shadow -->
            <float n="2">0</float>        <!-- for the shadow -->
            <float n="3" alias="/controls/lighting/panel-norm"/>     <!-- rembrandt cockpit (flightdeck) lights -->
	    <float n="4" alias="/instrumentation/doors/crew/position-norm"/>
            <float n="5" alias="/instrumentation/doors/passenger/position-norm"/>
            <string n="0" type="string"></string> <!-- Dual Control analog TDM-mpp1 (magnetos, flaps etc.) -->
            <string n="1" type="string"></string> <!-- Dual Control analog TDM-mpp2 only pilot to copilot (fuel in tank etc.) -->

          </generic>
        </multiplay>

        <menubar>
            <default>
                <menu n="100">
                    <label>Lockheed L-188</label>
                    <enabled type="bool">true</enabled>
                    <item>
                        <label>Autostart</label>
                        <binding>
                            <command>property-toggle</command>
                            <property>sim/model/autostart</property>
                        </binding>
                    </item>
                    <item>
                        <label>Select Livery</label>
                        <binding>
                            <command>nasal</command>
                            <script>aircraft.livery.dialog.open()</script>
                        </binding>
                    </item>
                </menu>
            </default>
          </menubar>

          <flaps>
              <setting>0.0</setting>
              <setting>0.2</setting>
              <setting>0.4</setting>
              <setting>0.6</setting>
              <setting>0.8</setting>
              <setting>1.0</setting>
          </flaps>

          <weight n="0">
              <name>Crew</name>
              <weight-lb>500</weight-lb>
              <max-lb>800</max-lb>
              <min-lb>400</min-lb>
          </weight>

          <weight n="1">
              <name>Pax/Baggage</name>
              <weight-lb>15000</weight-lb>
              <max-lb>30000</max-lb>
              <min-lb>0</min-lb>
          </weight>

    </sim>


    <input>
      <keyboard include="Systems/Lockheed-L-188-keyboard.xml"/>
    </input>

    <autopilot>
      <settings>
          <target-speed-kt type="double">220</target-speed-kt>
          <target-pitch-deg type="double">0</target-pitch-deg>
          <target-altitude-ft type="int">0</target-altitude-ft>
          <vertical-speed-fpm type="int">0</vertical-speed-fpm>
          <heading-bug-deg type="int">0</heading-bug-deg>
      </settings>
      <switches>
        <ap type="bool">0</ap>
        <hdg type="bool">0</hdg>
        <alt type="bool">0</alt>
        <ias type="bool">0</ias>
        <nav type="bool">0</nav>
        <appr type="bool">0</appr>
        <gps type="bool">0</gps>
        <hdg type="bool">0</hdg>
      </switches>    
    </autopilot>

    <controls>
        <gear>
            <brake-parking>1</brake-parking>
        </gear>
        <engines>
            <engine n="0">
                <selected type="bool">1</selected>
            </engine>
            <engine n="1">
                <selected type="bool">1</selected>
            </engine>
            <engine n="2">
                <selected type="bool">1</selected>
            </engine>
            <engine n="3">
                <selected type="bool">1</selected>
            </engine>
            <reverser_allow type="bool">0</reverser_allow>
        </engines>
    </controls>

    <consumables>
        <fuel>
            <tank n="0">
                <name>#1 Main</name>
                <selected type="bool">1</selected>
            </tank>
            <tank n="1">
                <name>#2 Main</name>
                <selected type="bool">1</selected>
            </tank>
            <tank n="2">
                <name>#1 Alt</name>
                <selected type="bool">1</selected>
            </tank>
            <tank n="3">
                <name>#2 Alt</name>
                <selected type="bool">1</selected>
            </tank>
            <tank n="4">
                <name>#3 Alt</name>
                <selected type="bool">1</selected>
            </tank>
            <tank n="5">
                <name>#4 Alt</name>
                <selected type="bool">1</selected>
            </tank>
            <tank n="6">
                <name>#3 Main</name>
                <selected type="bool">1</selected>
            </tank>
            <tank n="7">
                <name>#4 Main</name>
                <selected type="bool">1</selected>
            </tank>
        </fuel>
    </consumables>

<instrumentation include="Systems/Lockheed-L-188-init-instrumentation.xml"/>

<nasal>
	<doors>
          <file>Aircraft/Lockheed-L-188/Nasal/doors.nas</file>
        </doors>
     <L-188>
        <file>Aircraft/Lockheed-L-188/Nasal/L188.nas</file>        
	<file>Aircraft/Lockheed-L-188/Nasal/Electrical.nas</file>
        <file>Aircraft/Lockheed-L-188/Nasal/dc6-mk-prop-rev.nas</file>   
        <file>Aircraft/Lockheed-L-188/Nasal/mk-ki266.nas</file>
        <script>
	  <![CDATA[ki266.new(0);]]>
	</script>
        <file>Aircraft/Lockheed-L-188/Nasal/mk-agl-radar.nas</file>
        <file>Aircraft/Lockheed-L-188/Nasal/mk-ap-helper.nas</file>
        <file>Aircraft/Lockheed-L-188/Nasal/mk-swap-nav.nas</file>
        <file>Aircraft/Lockheed-L-188/Nasal/mk-rmi-adf-nav.nas</file>
        <file>Aircraft/Lockheed-L-188/Nasal/mk-frw.nas</file>
	<file>Aircraft/Lockheed-L-188/Nasal/RDR160.nas</file>
     </L-188>  
     <m877>
       <file>Aircraft/Instruments-3d/clock/M877/M877.nas</file>
    </m877>   
</nasal>
</PropertyList>
